[alias]
    ci = commit
    co = checkout
    di = diff
    br = branch
    me = merge
    st = status -s
    unstage = reset HEAD
    last = log -1 HEAD
    update-all-submodules = submodule foreach git pull origin master
[core]
    editor = vim
[merge]
    tool = vimdiff

