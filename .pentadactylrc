set! browser.tabs.closeWindowWithLastTab=false
set autocomplete=
" c compose
" j older, k newer
" o open
" e archive
" r reply
" a reply to all
" [ archive and previous, ] archive and next
set passkeys+=mail.google.com:cjkoera[]
set passkeys+=google.com/reader:npr
set passkeys+=docs.google.com/viewer:jk
set passkeys+=encrypted.google.com/search:jko/
set hintkeys=fghjklrtyui
set hlfind
set incfind
set defsearch=g
map < :tabm -1<cr>
map > :tabm +1<cr>
colorscheme simple_dark

